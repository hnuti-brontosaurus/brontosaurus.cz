{
  "name": "HB Codespaces",
  "dockerComposeFile": "docker-compose.yml",
  "service": "devcontainer",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",

  "postStartCommand": "bash /workspaces/${localWorkspaceFolderBasename}/.devcontainer/init.sh",


  "features": {},
  "customizations": {},

  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": {
      "label": "HB",
      "onAutoForward": "openBrowser"
    }
  }
}